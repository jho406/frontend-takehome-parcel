{
  "name": "frontend-takehome-parcel",
  "version": "0.0.0",
  "description": "Teachable frontend takehome with Parcel.",
  "main": "server.js",
  "scripts": {
    "build": "./node_modules/.bin/parcel build ./index.html",
    "dev": "concurrently './node_modules/.bin/parcel ./index.html' 'node ./server'",
    "start": "NODE_ENV=production node ./server",
    "test": "./node_modules/.bin/jasmine",
    "e2e": "concurrently './node_modules/.bin/parcel ./index.html' 'node ./server' './node_modules/.bin/cypress open'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UseFedora/frontend-takehome-parcel.git"
  },
  "author": "Teachable",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/UseFedora/frontend-takehome-parcel/issues"
  },
  "homepage": "https://github.com/UseFedora/frontend-takehome-parcel#readme",
  "dependencies": {
    "boostrap": "^2.0.0",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "fetch-mock": "^7.3.9",
    "images": "^3.0.2",
    "lodash": "^4.17.15",
    "normalize": "^0.3.1",
    "normalize.css": "^8.0.1",
    "parcel": "^1.9.7",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-truncate": "^2.4.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "canvas": "^2.6.0",
    "cypress": "^3.4.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "ignore-styles": "^5.0.1",
    "jasmine": "^3.5.0",
    "jasmine-enzyme": "^7.1.1",
    "jasmine-expect": "^4.0.3",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "node-fetch": "^2.6.0",
    "parcel-bundler": "^1.12.3",
    "sinon": "^7.5.0"
  }
